<%= form_for(@profile) do |f|%>
	<div class="field">
	  <%= f.label :first_name %><br>
	  <%= f.text_field :first_name %>
	</div>
	<div class="field">
	  <%= f.label :last_name %><br>
	  <%= f.text_field :last_name %>
	</div>
	<div class="field">
	  <%= f.label :phone %><br>
	  <%= f.text_field :phone %>
	</div>
	<div class="field">
	  <%= f.label :address %><br>
	  <%= f.text_field :address %>
	</div>
	<div class="actions">
	  <%= f.submit 'submit' %>
	</div>
<% end %>