<h2>Join the Dr. Roebuck's Family</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <%= text_field_tag 'first_name', nil, placehodler: 'First'%>
  <%= text_field_tag 'last_name', nil, placehodler: 'Last'%>
  
    <%= f.email_field :email, autofocus: true, placehodler: 'Email Address' %>
  
  
    <%= email_field_tag 'email_confirm', nil, placehodler: 'Repeat Email Address' %>
  

  
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    <%= f.password_field :password, autocomplete: "off", placehodler: 'Password' %>
  

  
    <%= f.password_field :password_confirmation, autocomplete: "off", placehodler: 'Repeat Password' %>
  

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
