<p id="notice"><%= notice %></p>

<h1>Listing Skin Diets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skin_diets.each do |skin_diet| %>
      <tr>
        <td><%= skin_diet.title %></td>
        <td><%= link_to 'Show', skin_diet %></td>
        <td><%= link_to 'Edit', edit_skin_diet_path(skin_diet) %></td>
        <td><%= link_to 'Destroy', skin_diet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skin diet', new_skin_diet_path %>
